{
  "compilerOptions": {
    "target": "ES6",
    "lib": ["es6", "dom"],
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "types": ["cypress", "node"]
  },
  "include": ["cypress/**/*.ts", "cypress/cypress.config.js"]
}
